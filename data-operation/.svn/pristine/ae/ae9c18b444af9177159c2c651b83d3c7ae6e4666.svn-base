<!DOCTYPE html>
<html lang="zh-CN">
<head>
    @@include('../base/header.html')
    <link href="export.css" rel="stylesheet">
    <title>导出楼栋</title>
</head>
<body>
<div id="buildingExport_wrapper">
    <form class="layui-form" action="">
        <div class="layui-form-item">
            <label class="layui-form-label">导出属性</label>
            <input type="checkbox" lay-skin="primary" id="c_all" lay-filter="c_all" title="全选">
            <div class="layui-input-block" id="column">
                <div class="layui-row">
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="楼栋名" value="name" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="楼栋别名" value="alias" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="单元数" value="unitNo" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="地上总楼层" value="floorOverGroundNo" lay-filter="column">
                    </div>
                </div>
                <div class="layui-row">
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="地下总楼层" value="floorUnderGroundNo" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="总楼层" value="floorTotalNo" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="标准层室号数" value="roomNoPerFloor" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="房号总数" value="roomNo" lay-filter="column">
                    </div>
                </div>
                <div class="layui-row">
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="竣工日期" value="builtDate" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="电梯数" value="liftNo" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="电梯品牌" value="liftBrand" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="详细地址" value="address" lay-filter="column">
                    </div>
                </div>
                <div class="layui-row">
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="建筑结构" value="structure" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="建筑类别" value="buildingType" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="占地面积" value="landArea" lay-filter="column"><br>
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="住宅品质" value="quality" lay-filter="column">
                    </div>
                </div>
                <div class="layui-row">
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="最新均价" value="avgPrice" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="价格系数" value="priceCoe" lay-filter="column"><br>
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="外墙装饰" value="wallDecoration" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="公共装修档次" value="publicDecoration" lay-filter="column"><br>
                    </div>
                </div>
                <div class="layui-row">
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="非标准层说明" value="nonStandDesc" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="价格差异说明" value="priceDesc" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="房号基准价" value="basePrice" lay-filter="column"><br>
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="房号基准价室号" value="baseRoomNo" lay-filter="column">
                    </div>
                </div>
                <div class="layui-row">
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="有无电梯" value="haveLift" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="主力面积" value="mainAcreage" lay-filter="column">
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="主力品质的建筑类别" value="mainCategory" lay-filter="column"><br>
                    </div>
                    <div class="layui-col-xs2">
                        <input type="checkbox" lay-skin="primary" name="column" title="特殊说明" value="priceDesc" lay-filter="column">
                    </div>
                </div>
            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-input-block">
                <button type="reset" class="layui-btn layui-btn-sm layui-btn-primary">重置</button>
                <button type="button" class="layui-btn layui-btn-sm" id="export" lay-filter="save">导出</button>
            </div>
        </div>
    </form>
</div>

@@include('../base/footer.html')
<script type="text/javascript" src="export.js"></script>
</body>
</html>