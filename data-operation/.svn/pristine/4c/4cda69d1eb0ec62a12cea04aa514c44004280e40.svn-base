package com.hifo.dataoperation.service.coe.enums;
/**
 * 
 * @author 杨捷
 * @Date 2019年4月28日
 * @Description 系数结构体条件类型
 */
public enum CoeStructureConditionType {
	/**
	 * 整体区域
	 */
	WHOLE_AREA(1, "整体区域"),
	/**
	 * 物业品质
	 */
	QUALITY(2, "物业品质"),
	/**
	 * 有无电梯
	 */
	HAS_Elevator(3, "有无电梯"),
	/**
	 * 建筑类别
	 */
	BUILDING_TYPE(4, "建筑类别"),
	/**
	 * 建成年代
	 */
	BUILDING_YEARS(5, "建成年代"),
	/**
	 * 主力面积
	 */
	MAIN_FLOOR_SPACE(6, "主力面积");

	private Integer code;
	private String value;

	private CoeStructureConditionType(Integer code, String value) {
		this.code = code;
		this.value = value;
	}

	public Integer getCode() {
		return code;
	}

	public void setCode(Integer code) {
		this.code = code;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}
