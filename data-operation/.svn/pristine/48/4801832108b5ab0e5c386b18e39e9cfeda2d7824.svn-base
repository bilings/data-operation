#homeIndex_wrapper {
    min-width: 1000px;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

#homeIndex_wrapper #env {
    position: absolute;
    top: 0;
    left: 60px;
}

