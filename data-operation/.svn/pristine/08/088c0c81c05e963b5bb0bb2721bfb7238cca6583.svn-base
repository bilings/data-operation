/*全局设定，输入框的大小*/
.layui-form-label {
    padding: 5px 15px;
}
.layui-input, .layui-select, .layui-textarea {
    height: 30px;
    line-height: 30px;
}
.layui-form-select dl dd, .layui-form-select dl dt {
    line-height: 30px;
}
.layui-input-inline input{
    height: 30px;
    line-height: 30px;
}
#districtDiv #buildingCategoryDiv{
    width: 6% !important;
}
#timeScope {
    width: 10% !important;
}

.layui-input-inline input{
    margin-right: 20px; !important;
}
.layui-form-item {
    height: 30px;
    line-height: 30px;
    margin-bottom: 0px; !important;
}
.layui-table {
    margin-top: 0px !important;
}
#estateNameDiv {
    width: 12% !important;
}
.caseNo{
    width: 7% !important;
}
.gain{
    width: 6% !important;
}
.selectDiv{
    width: 7% !important;
}
